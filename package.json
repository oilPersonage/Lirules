{
  "name": "Lirules-front",
  "version": "1.0.2",
  "private": false,
  "files": [
    "build"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "start": "webpack-dev-server --progress --mode=development",
    "build": "webpack --mode=production ",
    "build:dev": "webpack --mode=development",
    "deploy": "gh-pages -d build",
    "analyze": "webpack --mode=production --env.analyze=true",
    "lint": "npm-run-all --parallel lint:*",
    "lint:ts": "eslint \"src/**/*.ts{,x}\"",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint-fix": "npm-run-all format lint-fix:*",
    "lint-fix:ts": "eslint \"src/**/*.ts{,x}\" --fix",
    "lint-fix:css": "stylelint \"src/**/*.scss\" --fix",
    "lint:lang": "npm run cli lang-lint",
    "format": "prettier --write \"src/*.{json,{j,t}s{,x},scss,}\"",
    "test": "jest",
    "watch-tests": "jest --watch",
    "coverage": "jest --coverage",
    "cli": "vtb-ib-cli",
    "version": "npm run cli changelog && git add CHANGELOG.md",
    "release": "npm run cli release -- --from release/7.2-2022.8",
    "pre-release": "npm run cli release -- --pre \"--preid\" \"rc\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently npm:lint",
      "pre-push": "npm run cli commit-lint -- --from release/7.2-2022.8"
    }
  },
  "lang-lint": {
    "ignore": [
      "webpack/webpack.config.js",
      "constants.js"
    ]
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@reduxjs/toolkit": "^1.8.5",
    "@svgr/webpack": "^6.3.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/classnames": "2.3.0",
    "@types/jest": "29.0.0",
    "@types/react-redux": "7.1.24",
    "@types/webpack-env": "1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/eslint-plugin-tslint": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@webpack-cli/serve": "^1.7.0",
    "axios": "^0.27.2",
    "babel-jest": "29.0.2",
    "babel-loader": "^9.1.0",
    "babel-plugin-import": "1.13.5",
    "concurrently": "7.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "8.22.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.1",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.31.6",
    "eslint-plugin-react-hooks": "4.6.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "generate-changelog": "1.8.0",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.0.2",
    "jest-cli": "29.0.2",
    "jest-watch-typeahead": "2.1.1",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "4.1.5",
    "portscanner": "^2.2.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "pretty-quick": "3.1.3",
    "react-refresh": "^0.14.0",
    "react-refresh-typescript": "^2.0.7",
    "redux-saga-test-plan": "^4.0.5",
    "sass": "1.54.8",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "14.11.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-rational-order": "^0.0.4",
    "stylelint-config-standard": "28.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.5.0",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.3.0",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.3.1",
    "tslint": "6.1.3",
    "tslint-react": "5.0.0",
    "type-fest": "^3.5.0",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.1"
  },
  "dependencies": {
    "@babel/plugin-transform-object-assign": "^7.18.6",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@tanstack/react-query": "^4.29.7",
    "bundle-loader": "^0.5.6",
    "classnames": "2.3.1",
    "dayjs": "1.11.5",
    "framer-motion": "^7.3.2",
    "gsap": "^3.11.5",
    "lodash": "^4.17.21",
    "npm": "^8.19.1",
    "rc-resize-observer": "^1.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.10.0",
    "redux": "4.2.0",
    "redux-devtools-extension": "2.13.8",
    "redux-saga": "1.2.1",
    "redux-saga-promise-actions": "^1.3.5"
  }
}
